<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Edit Company Page</h1>
    <form action="/companies/<%= company._id%>?_method=put" method="POST">
      <label for="name">Name</label>
      <input type="text" name="name" id="name" value="<%= company.name%>" /><br>
      <label for="logo">Logo</label>
      <input type="text" name="logo" id="logo" value="<%= company.logo %>" /><br>
      <label for="address">Address</label>
      <input type="text" name="address" id="address" value="<%= company.address%>" /><br>
       <label for="city">city</label>
      <input type="text" name="city" id="city" value="<%= company.city %>" /><br>
      <label for="telephone">phone</label>
      <input type="text" name="telephone" id="telephone" value="<%= company.telephone %>" /><br>
      <input type="submit" value="Create Company">
    </form>
  </body>
</html>